<div>
    <title>“Web”</title>
    {{#with web}}
 <main>
   <section>
      {{> title big=true data="Web"}}
     <ul>
        {{#each languages}}
          <li>
            {{name}}
            <br>
            {{version}}
            <br>
            {{description}}
          </li>
            </ul>
        {{/each}}
        </section>
    <hr>
    <br>
    <em><mark>follow the news...</mark></em>
    
    <title>“Program”</title>
    {{#with program}}
   <section>
      {{> title big=true data="Excursion"}}
     <ul>
        {{#with excursion}}
          <li>
            {{name}}
          </li>
            {{#with location}}
            <li>
            {{city}}
            {{country}}
            </li>
            {{/with}}
            </ul>
        {{/with}}
        </section>
    <hr>
    <br>
</main>
 {{/with}}
{{/with}}
</div>

   